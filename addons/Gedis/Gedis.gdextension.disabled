linux.x86_64.release = "res://addons/Gedis/bin/libgedis.so"
linux.arm64 = "res://addons/Gedis/bin/libgedis.so"
linux.arm64.debug = "res://addons/Gedis/bin/libgedis.so"
linux.arm64.release = "res://addons/Gedis/bin/libgedis.so"

