linux = "res://addons/Gedis/bin/libgedis.so"
linux.debug = "res://addons/Gedis/bin/libgedis.so"
linux.release = "res://addons/Gedis/bin/libgedis.so"
linuxbsd = "res://addons/Gedis/bin/libgedis.so"
linuxbsd.debug = "res://addons/Gedis/bin/libgedis.so"
linuxbsd.release = "res://addons/Gedis/bin/libgedis.so"

# Linux (architectures)
linux.x86_64 = "res://addons/Gedis/bin/libgedis.so"
